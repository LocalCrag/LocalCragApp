{
  "name": "localcrag-client",
  "version": "1.25.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "i18n:extract": "transloco-keys-manager extract -d \"\" -l de -R",
    "i18n:find": "transloco-keys-manager find",
    "cypress": "cypress open",
    "cypress:run-test-backend": "cd ../server/src && export PYTHONPATH=. && export LOCALCRAG_CONFIG=config/test-ci-e2e.cfg && pipenv run flask run",
    "e2e:headless": "cd ../scripts && bash run-cypress-with-dependencies.sh",
    "e2e": "cd ../scripts && CYPRESS_MODE=open bash run-cypress-with-dependencies.sh",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org localcrag --project localcrag-client ./dist/client && sentry-cli sourcemaps upload --org localcrag --project localcrag-client ./dist/client",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@jsverse/transloco": "^8.1.0",
    "@jsverse/transloco-messageformat": "^8.1.0",
    "@ngrx/effects": "^21.0.1",
    "@ngrx/store": "^21.0.1",
    "@primeuix/themes": "^2.0.3",
    "@sentry/angular": "^10.38.0",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^3.3.1",
    "jwt-decode": "^4.0.0",
    "konva": "^9.3.18",
    "maplibre-gl": "^5.1.1",
    "ngx-colors": "^3.6.0",
    "ngx-infinite-scroll": "^21.0.0",
    "ngx-mapbox-gl": "^14.0.0",
    "ngx-matomo-client": "^9.0.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "quill": "^1.3.7",
    "quill-blot-formatter": "^1.0.5",
    "quill-image-uploader": "^1.3.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.2.0",
    "@angular-eslint/eslint-plugin": "21.2.0",
    "@angular-eslint/eslint-plugin-template": "21.2.0",
    "@angular-eslint/schematics": "21.2.0",
    "@angular-eslint/template-parser": "21.2.0",
    "@angular/build": "^21.1.3",
    "@angular/cli": "~21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@jsverse/transloco-keys-manager": "^7.0.2",
    "@ngrx/store-devtools": "^21.0.1",
    "@sentry/cli": "^2.42.2",
    "@types/geojson": "^7946.0.16",
    "@types/jasmine": "~5.1.8",
    "@types/mapbox-gl": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "cypress": "^13.6.6",
    "eslint": "^9.38.0",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.3.3",
    "typescript": "~5.9.0"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write .",
      "eslint"
    ],
    "src/assets/i18n/**/*.{json}": [
      "prettier --write ."
    ]
  }
}
